@charset "utf-8";
/* Reset.CSS Document */
html,body{margin:0;padding:0;}
h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,cite,code,del,dfn,em,img,q,s,samp,small,strike,strong,sub,sup,tt,var,dd,dl,dt,li,ol,ul,fieldset,form,label,legend,button,table,caption,tbody,tfoot,thead,tr,th,td
{margin:0;padding:0;border:0;font-weight:normal;font-style:normal;font-size:100%;line-height:1;font-family:inherit;}
table{border-collapse:collapse;border-spacing:0;}
ol,ul{list-style:none;}
q:before,q:after,blockquote:before,blockquote:after{content:"";}
html{overflow-y:scroll;font-size:100%;-webkit-text-size-adjust:100%;-ms-text-size-adjust:100%;}
a:focus{outline:thin dotted;}
a:hover,a:active{outline:0;}
article,aside,details,figcaption,figure,footer,header,hgroup,nav,section{display: block;}
audio,canvas,video{display:inline-block;*display:inline;*zoom:1;}
audio:not([controls]){display:none;}
sub,sup{font-size:75%;line-height:0;position:relative;vertical-align:baseline;}
sup{top:-0.5em;}
sub{bottom:-0.25em;}
img{border:0;-ms-interpolation-mode:bicubic;}
button,input,select,textarea{font-size:100%;margin:0;vertical-align:baseline;*vertical-align:middle;}
button,input{line-height:normal;*overflow:visible;}
button::-moz-focus-inner,input::-moz-focus-inner{border:0;padding:0;}
button,input[type="button"],input[type="reset"],input[type="submit"]{cursor:pointer;-webkit-appearance:button;}
input[type="search"]{-webkit-appearance:textfield;-webkit-box-sizing:content-box;-moz-box-sizing:content-box;box-sizing:content-box;}
input[type="search"]::-webkit-search-decoration{-webkit-appearance:none;}
textarea{overflow:auto;vertical-align:top;}

/* CSS Document */


/*top --------------------------------------------------------------------------------------------------------------------------------------------------------------*/
.top { position:relative; /*width:1685px;*/min-width:1240px; margin:0 auto; height:233px; }
/*search*/
#search { position:absolute; top:80px; right:25px; }
.search_txt { width:234px; height:34px; }
#search_txt { width:185px; height:24px; margin:5px 15px; border:none; background:none; font:14px "SimSun"; color:#898989; *margin-top:4px; line-height:24px\9; _overflow:hidden;}
#search_txt:focus{ outline:none; border:none;}
#search_btn { position:absolute; top:11px; right:15px; height:13px; width:13px; background:none; background:url(<%= asset_path "btn_search_blue.png" %>) no-repeat; border:none; _overflow:hidden;}

/*head title*/
.top .title { float:left; width:636px;_width:626px; height:143px;padding:90px 0 0 0;}
.top .title li { float:left; padding:5px 0 5px 45px; font:42px "Microsoft Yahei";}
.def_gray { background:url(<%= asset_path "icon_blue.png" %>) no-repeat 25px -22px;_background-position:25px -27px; }
.top .title li a{text-decoration:none; color:#b6b6b6; }
.top .title li.li_select{ padding-left:10px;}
.top .title li select { font-size:12px; position:relative; top:-6px; /*top:30px\0; */*top:8px!important;_top:5px;}
.top .title li select option{ padding:0 5px;}
/*user_nav*/
.user_nav_box{ float:right; width:604px;padding:110px 0 0 0px;}
.user_nav { float:right;min-width:340px; height:123px;}
.user_nav li { float:left; padding:10px 5px; margin-right:5px; }
.user_nav li.settings { padding-left:20px; }
.user_nav li a { font:14px "SimSun"; text-decoration:none; display:block; padding:5px; }

/*tab content --------------------------------------------------------------------------------------------------------------------------------------------------------------*/
/*tab_links*/
.tab_nav{ position:relative; /*width:1685px;*/min-width:1240px;_width:100%; margin:0 auto;}
.tabs_links { position:absolute; bottom:0px;_bottom:6px;/*border-bottom:1px solid #69c0e0; */padding-left:1%; width:99%;}
.tabs_links ul { display:inline;}
.tabs_links ul li { display:inline;}
.tabs_links ul li a {float:left;/*width:135px;*/ height:30px; padding:6px 15px; text-decoration:none; text-align:center; text-shadow: 1px 1px 1px #fff; font:18px/30px "Microsoft Yahei"; margin-right:5px; -moz-border-radius:5px 5px 0px 0px; -webkit-border-radius:5px 5px 0px 0px; -khtml-border-radius:5px 5px 0px 0px; border-radius:5px 5px 0px 0px; position:relative; }
/*.tabs_links ul li span { background-color:#6f91b1; padding:1px 5px 1.6px 5px; color:#FFFFFF; text-shadow: 0px 0px 0px #fff; border:1px solid #57748d; font-size:10px; position:absolute; -moz-border-radius:100px; -webkit-border-radius:100px; -khtml-border-radius:100px; border-radius:100px; bottom:20px; right:2px; }
.tabs_links ul li.active a span { bottom:21px; }
*/
.tabs_links ul li a:hover { background:#fff!important; }
.tabs_links ul li.active a { background:#fff!important; border-bottom:#fff; padding:6px 15px 7px 15px; text-align:center; }
/*tab_content*/
.main { /*width:1685px;*/min-width:1240px; margin:0 auto;}
.content{ float:left; min-height:770px;_height:770px; width:100%;}
.tab_content{ width:94%;margin:30px auto;}
  /*user_area*/
.user_area{ height:95px; padding:0 20px; _overflow:hidden;}
.user_img,img.con_img{ float:left; padding:1px;/* margin-top:13px;*/ margin-right:20px; border:1px solid #dddddd; background:#ffffff;}
.user_info{ float:left; padding:11px 0; width:280px; height:72px; margin:1px 30px 0 0; color:#5c5c5c;}
.user_info h4{font:18px/30px "Microsoft Yahei";}
.user_info p{font:12px/18px "Microsoft Yahei";}
  /*con_show——fame con*/
.con_show{ overflow:hidden;}
.con{ height:192px;}
.con,.con_bp{ float:left; width:49%; margin-top:30px; margin-right:1%; padding-top:1px; -moz-border-radius:5px; -webkit-border-radius:5px; -khtml-border-radius:5px; border-radius:5px; _overflow:hidden;}
.con.nomargin_r,.con_bp.nomargin_r{ float:right!important;}
.con h2{ height:40px;font:14px/40px "Microsoft Yahei"; padding:0 20px; margin-bottom:30px;}
img.con_img{margin-left:20px; display:inline; }
.article{ float:right; height:119px; width:75%;}
.article h3{ font:bold 13px/24px "Microsoft Yahei"; color:#000;}
.article p{ color:#898989;font:12px/18px "SimSun"; height:70px; padding-right:30px;overflow:hidden;}


/*.con_bp{ float:left; width:782px; margin:30px 37px 0 0; padding-top:1px; -moz-border-radius:5px; -webkit-border-radius:5px; -khtml-border-radius:5px; border-radius:5px; _overflow:hidden;}*/
.con_bp h2{height:46px;font:14px/46px "Microsoft Yahei"; padding:0 40px; -moz-border-radius:5px; -webkit-border-radius:5px; -khtml-border-radius:5px; border-radius:5px;}
.con_bp table{ width:95%; margin:0 auto;}
.con_bp table td,.con_bp table th{ height:42px; text-align:center; vertical-align:middle; font:14px "Microsoft Yahei"; color:#000;}

.btn_operate a{ padding:3px 8px;_padding:4px 8px 2px; display: inline-block; min-width:30px; margin:5px 4px; color:#fff!important; text-decoration:none; font:14px/24px "SimSun";}
.btn_more{ float:right; padding:5px; text-decoration:none; color:#898989;font:12px "SimSun"; text-transform:uppercase;}
.con_bp .btn_more{font:14px/34px "Microsoft Yahei"; position:relative; right:-30px;}
/*news*/
.news{ margin-top:65px;}
.left_menu{ float:left; height:510px; width:18%;}
.left_menu h2{ min-height:46px; font:28px/46px "Microsoft Yahei"; margin-bottom:24px;}
.left_menu ul{ height:420px;/*margin:0px 27px; */padding:20px 27px 0;}
.left_menu ul li{ padding:5px 0px 5px 20px;}
.left_menu ul li a{ text-decoration:none;font:18px/40px "Microsoft Yahei"; }
.right_main{ float:right; position:relative; width:80%; min-height:555px;_height:555px;}
.right_main .info_list{ width:100%;}
.right_main .info_list tr{ cursor:pointer;}
.right_main .info_list th{font:14px "Microsoft Yahei"; height:46px;}
.right_main .info_list td{ height:45px; font:14px "SimSun"; color:#5c5c5c; text-align:center;}
.right_main .info_list td a{color:#5c5c5c; text-decoration:none;}
tr.noborder td{ border:0!important;}

.page_bar{ text-align:center; border:0!important; -moz-border-radius:5px; -webkit-border-radius:5px; -khtml-border-radius:5px; border-radius:5px;}
.page_bar a{ margin:0 10px;}
.page_bar span a{ background:none!important;margin:0 5px;}
.page_bar select{margin:0 40px 0 10px;}
/*news inner*/
.wizard{ position:absolute; width:97%; top:-42px; height:42px; font:14px/42px "Microsoft Yahei"; color:#5c5c5c; padding-left:3%;}
.wizard a{ color:#5c5c5c; text-decoration:none;}
span.btn_operate{ float:right; /*margin-top:10px;*/}
div.page_bar{ padding:10px 0;}
.inner_tit{ padding:0 5%; height:46px; font:14px/46px "Microsoft Yahei"; -moz-border-radius:5px; -webkit-border-radius:5px; -khtml-border-radius:5px; border-radius:5px;}
.inner_tit span{ float:right;}
.inner_tit table,.intr table{height:46px;width:100%;}
.inner_tit table td,.intr table td{width:25%;}
.inner_main{ padding:0 0 0 5%;}
.intr{ font:14px/46px "Microsoft Yahei";}
.intr span{ float: right;}
.inner_main p{ font:12px/28px "SimSun";}
.btn_review{margin:10px 0;}
.inner_main p img{margin: 10px 20px 15px 0px;padding:1px;border:1px solid #dddddd;}

.review{ padding:0 0 0 5%;font:12px/28px "SimSun";margin-bottom:30px;}
.review p{line-height:24px;}
.review p a{text-decoration:none;color:#5c5c5c;}
.review .review_intr{ width:100%; margin:10px 0px;}
.review .review_intr td.btn_operate{text-align: right;}
.inner_article,.intr{padding-right:5%;}
/*story*/
.story_main{ padding-top:40px;}
/*news publish*/
.page_bar input,.pop_review form .review_btn input{ border:none; padding:5px 25px; color:#fff; margin:0 10px; font:14px "Microsoft Yahei";}
#form_main h3.inner_tit{ text-align: center;}
